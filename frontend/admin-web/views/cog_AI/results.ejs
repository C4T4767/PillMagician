<body>
    <div class="container">
        <h1 class="text-center mt-4 mb-4">분류 결과</h1>
        <div class="row">
            <% items.forEach(function(item, index) { %>
            <div style="height: 200px; border:2px solid <%= index === 0 ? '#ff0000' : '#000000' %>;">
                <div class="container d-flex justify-content-between">
                    <div class="col-md-5">
                        <img src="<%= item.item_image %>" class="card-img-top" alt="<%= item.item_name %>"style="max-width: 100%; max-height: 196px;">
                    </div>
                    <div class="col-md-6">
                        <h5 class="card-title"><%= item.item_name %></h5>
                        <p class="card-text">확률: <%= item.probability %></p>
                    </div>
                </div>
            </div>
            <% }); %>
        </div>
    </div>
</body>
