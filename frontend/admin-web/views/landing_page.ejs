    <script>
        document.addEventListener('DOMContentLoaded', function() {
            var downloadLink1 = document.getElementById('downloadLink1');
            var downloadLink2 = document.getElementById('downloadLink2');

            function setupDownload(link) {
                link.addEventListener('click', function(event) {
                    event.preventDefault();  // 기본 동작을 막습니다.

                    // 파일 URL과 파일 이름을 설정합니다.
                    const fileUrl = 'asset/app-release.apk';
                    const fileName = 'app-release.apk';

                    // a 태그를 생성하여 다운로드 속성을 설정합니다.
                    const a = document.createElement('a');
                    a.href = fileUrl;
                    a.download = fileName;

                    // a 태그를 클릭하여 파일을 다운로드합니다.
                    document.body.appendChild(a);
                    a.click();
                    document.body.removeChild(a);
                });
            }

            if (downloadLink1) {
                setupDownload(downloadLink1);
            } else {
                console.error('Element with id "downloadLink1" not found');
            }

            if (downloadLink2) {
                setupDownload(downloadLink2);
            } else {
                console.error('Element with id "downloadLink2" not found');
            }
        });
    </script>
